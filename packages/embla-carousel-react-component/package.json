{
  "name": "embla-carousel-react-component",
  "version": "0.5.2",
  "author": "Xuan Hung <vu.xuanhung1509@gmail.com>",
  "description": "A lightweight carousel library for React, based on Embla Carousel",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuanhung1509/embla-carousel-react-component"
  },
  "bugs": {
    "url": "https://github.com/xuanhung1509/embla-carousel-react-component/issues"
  },
  "homepage": "https://github.com/xuanhung1509/embla-carousel-react-component/blob/master/packages/embla-carousel-react-component/README.md",
  "license": "MIT",
  "keywords": [
    "slider",
    "slideshow",
    "gallery",
    "carousel",
    "react"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepublishOnly": "yarn build",
    "prepack": "json -I -f package.json -e \"delete this.dependencies; delete this.devDependencies\""
  },
  "dependencies": {
    "embla-carousel-react": "^7.0.5"
  },
  "devDependencies": {
    "@types/node": "^18.14.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "eslint-config-custom": "*",
    "json": "^11.0.0",
    "tsconfig": "*",
    "typescript": "^4.9.3",
    "vite": "^4.2.1",
    "vite-plugin-dts": "^2.0.2"
  },
  "peerDependencies": {
    "react": "^16.8.0. || ^17.0.1 || ^18.0.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/embla-carousel-react-component.umd.js",
  "module": "./dist/embla-carousel-react-component.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/embla-carousel-react-component.es.js",
      "require": "./dist/embla-carousel-react-component.umd.js"
    }
